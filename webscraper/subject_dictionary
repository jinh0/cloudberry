num_subjects = 286
subject_list_1 = []
subject_list_final = []
dict_keys = []

# filter for subject codes
def subject_finder(list):
    for i in list:
        res_raw = i.split('Apply')
        subject_list_1.append(res_raw[0])

    for j in subject_list_1:
        res_final = j.rsplit(' ', 1)
        subject_list_final.append(res_final[0][5:])
    return subject_list_final

# pull first course code and make dict keys
def find_keys(list):
    for i in list:
        dict_keys.append(i[:4])
    return dict_keys

# append keys and subjects to dictionary
def make_dict(key_list, value_list):
    subjects = dict(zip(find_keys(key_list), subject_finder(value_list)))
    with open("subjects.json", "w") as outfile:
        json.dump(subjects, outfile, indent=2)
    return subjects

print(make_dict(subject_finder(subject_list_raw),find_keys(subject_list_raw)))



    



